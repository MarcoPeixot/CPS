{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/frontend"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/frontend/$1"
    },
    {
      "src": "/.*",
      "dest": "/dist/frontend/index.html"
    }
  ]
}