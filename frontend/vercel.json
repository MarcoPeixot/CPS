{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/frontend/$1",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    },
    {
      "src": "/.*",
      "dest": "/dist/frontend/index.html"
    }
  ]
}